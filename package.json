{
  "name": "cube-timer",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --port 4201 --configuration dev",
    "env:prod": "node scripts/environment.js",
    "prebuild": "npm run env:prod && npm run buildinfo",
    "build": "ng build --prod",
    "pretest": "npm run buildinfo",
    "test": "ng test",
    "pretest:headless": "npm run buildinfo",
    "test:headless": "ng test --watch false --progress false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:headless": "ng e2e --config=protractor-ci.conf.js",
    "deploy": "npm run build && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "buildinfo": "node scripts/build-info.js",
    "postpull": "npm install && npm run buildinfo",
    "precommit": "npm run lint && npm run test:headless && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.0",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/core": "7.0.0",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "7.0.0",
    "@angular/http": "7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/router": "7.0.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "core-js": "^2.4.1",
    "firebase": "^5.5.5",
    "firebase-key": "^2.0.1",
    "moment": "^2.22.2",
    "ngx-moment": "^3.1.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.2",
    "@angular/cli": "^7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@ngrx/schematics": "^6.1.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "coveralls": "^3.0.2",
    "dateformat": "^3.0.3",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "^5.4.1",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
